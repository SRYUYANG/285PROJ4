package util;

public class Speed
{
  private double xSpeed, ySpeed;
  public Speed(double _xSpeed, double _ySpeed)
  {
    xSpeed = _xSpeed;
    ySpeed = _ySpeed;
  }
  
  public double getX()
  {
    return corX;
  }
  
  public double getY()
  {
    return corY;
  }
  
  public void setX(double inX)
  {
    corX = inX;
  }
  
  public void setY(double inY)
  {
    corY = inY;
  }
}
