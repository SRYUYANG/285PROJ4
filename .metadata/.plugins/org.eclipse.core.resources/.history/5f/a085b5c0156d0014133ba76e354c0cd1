package util;

public class Location
{
  private double corX, corY;
  public Location(double inX, double inY)
  {
    corX = inX;
    corY = inY;
  }
  
  public double getX()
  {
    return corX;
  }
  
  public double getY()
  {
    return corY;
  }
  
  public void setX(int inX)
  {
    corX = inX;
  }
  
  public void setY(int inY)
  {
    corY = inY;
  }
}
