package util;

public class Location
{
  private double corX, corY;
  public Location(double inX, int inY)
  {
    corX = inX;
    corY = inY;
  }
  
  public int getX()
  {
    return corX;
  }
  
  public int getY()
  {
    return corY;
  }
  
  public void setX(int inX)
  {
    corX = inX;
  }
  
  public void setY(int inY)
  {
    corY = inY;
  }
}
